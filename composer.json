{
    "name": "simplex/simplex",
    "description": "Modular, micro framework for PHP 7",
    "keywords": ["modular", "micro framework"],
    "authors": [
        {
            "name": "Freddie Frantzen",
            "email": "freddie@freddiefrantzen.com"
        }
