        "ocramius/proxy-manager": "^2.1",
        "php-di/php-di": "^5.4",
        "relay/relay": "^1.1",
        "symfony/config": "^3.2",
        "symfony/console": "^3.2",
        "symfony/finder": "^3.2",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/routing": "^3.2",
        "vlucas/phpdotenv": "^2.4",
        "willdurand/hateoas": "^2.10",
        "zendframework/zend-diactoros": "^1.3",
        "zendframework/zend-config": "^3.1"
